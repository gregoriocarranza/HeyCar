{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/app/Index.jsx",
    "src/app/Register.jsx",
    "src/app/Login.jsx",
    "src/app/Home.jsx"
  ]
}
